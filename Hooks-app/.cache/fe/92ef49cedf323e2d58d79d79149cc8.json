.jsx","original":{"line":62,"column":1}},{"generated":{"line":81,"column":1},"source":"UseStateExample.jsx","original":{"line":3,"column":0}},{"generated":{"line":83,"column":15},"source":"UseStateExample.jsx","original":{"line":64,"column":15},"name":"UseStateExample"}],"sources":{"UseStateExample.jsx":"import React, { useState } from 'react';\n\nconst UseStateExample = () => {\n    const [showMessage, setShowMessage] = useState(false);\n    const [formValues, setFormValues] = useState({\n        name: '',\n        age: '',\n        fruit: ''\n    });\n\n    const handleChange = evt => {\n        const updateFormValues = {\n            ...formValues,\n            [evt.target.id]: evt.target.value\n        };\n\n        setFormValues(updateFormValues);\n    };\n\n    const handleSubmit = evt => {\n        evt.preventDefault();\n        setShowMessage(true);\n    };\n\n    return (\n        <>\n            <p>Complete the form below and see a nice message about yourself!</p>\n            <form onSubmit={handleSubmit}>\n                <label htmlFor='name'>What should we call you?</label>\n                <input id='name' value={formValues['name']} onChange={handleChange} />\n\n                <label htmlFor='age'>How old are you?</label>\n                <input id='age' value={formValues['age']} onChange={handleChange} />\n\n                <label htmlFor='fruit'>What's your favourite fruit?</label>\n                <select id='fruit' value={formValues['fruit']} onChange={handleChange}>\n                <option>Bananas</option>\n                <option>Apples</option>\n                <option>Oranges</option>\n                <option>Strawberries</option>\n                <option>Pears</option>\n                <option>Dragon fruit</option>\n                </select>\n\n                <button>Submit</button>\n            </form>\n            {showMessage && (\n                <div className='form-message'>\n                    <p>\n                    Hi <strong>{formValues['name']}</strong>, what a lovely name. And I\n                    can't believe you're only <strong>{formValues['age']}</strong> years\n                    old, that's no age!\n                    </p>\n                    <p>\n                    Your favourite fruit can't be <strong>{formValues['fruit']}</strong>\n                    , mine too!\n                    </p>\n                </div>\n            )}\n        </>\n    );\n};\n\nexport default UseStateExample;"},"lineCount":null}},"error":null,"hash":"50e3adf846cb1676bc8fa1d29daa8474","cacheData":{"env":{}}}